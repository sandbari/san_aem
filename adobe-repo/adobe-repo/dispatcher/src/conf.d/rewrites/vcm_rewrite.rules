##Rewrite rules for sitemap.xml, robots.txt and favicon
	RewriteRule ^/robots.txt$ /content/dam/vcm/configs/robots.txt [NC,PT]
	RewriteRule ^/favicon.ico$ /content/dam/vcm/configs/favicon.ico [NC,PT]
	RewriteRule ^/sitemap.xml$ /content/dam/vcm/configs/sitemap.xml [NC,PT]

##Added default headers for Post Login Integration
		#SetEnvIf Origin ^(https?://(?:.+\.)?(msweb\.vcm\.com|mysecure.vcm.com|postlogin.vcm.com|memberapi.vcm.com|investorapi.vcm.com)(?::\d{1,5})?)$   CORS_ALLOW_ORIGIN=$1
		SetEnvIf Origin ^(https?://(?:.+\.)?(msweb\.vcm\.com|mysecure.vcm.com|postlogin.vcm.com|memberapi.vcm.com|investorapi.vcm.com|vcm.com)(?::\d{1,5})?)$   CORS_ALLOW_ORIGIN=$1
		Header append Access-Control-Allow-Origin  %{CORS_ALLOW_ORIGIN}e   env=CORS_ALLOW_ORIGIN
		Header merge  Vary "Origin"
        Header always set Access-Control-Allow-Headers "Authorization"
        Header always set Access-Control-Allow-Methods "GET"
        Header always set Access-Control-Expose-Headers "Content-Security-Policy, Location"
        Header always set Access-Control-Max-Age "600"
		Header always set Access-Control-Allow-Credentials "true"
        <If "%{REQUEST_METHOD} = 'OPTIONS'">
        Header always set Access-Control-Allow-Origin "*"
        </If>
        RewriteCond %{REQUEST_METHOD} OPTIONS
        RewriteRule ^(.*)$ $1 [R=200,L]


##Remove .html if it comes in the url - excluding experience fragment urls
	RewriteCond %{REQUEST_URI} !^/vcm-experience-fragments/
	RewriteRule "^/(.*).html$" "https://%{SERVER_NAME}/$1" [L,R=301]

##Cookie based Home page redirections
	RewriteCond %{HTTP_COOKIE} ^.*user_entity_type=usaa_member.*$
	RewriteRule ^/?$ https://${VCM_INVESTOR_HOSTNAME}/member [L,R]

	RewriteCond %{HTTP_COOKIE} ^.*user_entity_type=financial_professional.*$
	RewriteRule ^/?$ https://${VCM_ADVISOR_HOSTNAME}/financial-professional [L,R]

	RewriteCond %{HTTP_COOKIE} ^.*user_entity_type=institutional_investor.*$
	RewriteRule ^/?$ https://${VCM_INSTITUTIONAL_HOSTNAME}/institutional-investor [L,R]

	RewriteCond %{HTTP_COOKIE} !^.*user_entity_type.*$
	RewriteRule ^/?$ /content/vcm/us/en.html [L,PT]

##Remove Trailing slash
	RewriteRule ^(.*)/+$ https://%{SERVER_NAME}$1 [L,R=301]

##Rewrite rule for experience fragments
    RewriteRule ^/vcm-experience-fragments/(.*)$  /content/experience-fragments/vcm-experience-fragments/$1 [L,PT]
    RewriteRule ^/content/experience-fragments/(.*)$  /content/experience-fragments/$1.html [L,PT]
    RewriteRule ^/content/experience-fragments/(.*)/~$  /content/experience-fragments/$1.html [L,R=301]

##vcm rewrites for pdf urls
	RewriteRule ^/assets/(.*)\.pdf$  /content/dam/vcm/$1.pdf [L,PT]

##vcm rewrites for other than /

    RewriteCond %{REQUEST_URI} !^$
    RewriteCond %{REQUEST_URI} !^/$
    RewriteCond %{REQUEST_URI} !^dispatcher/invalidate.cache$
    RewriteCond %{REQUEST_URI} !^/apps
    RewriteCond %{REQUEST_URI} !^/content
    RewriteCond %{REQUEST_URI} !^/etc
    RewriteCond %{REQUEST_URI} !^/libs
    RewriteCond %{REQUEST_URI} !^/tmp
    RewriteCond %{REQUEST_URI} !^/bin
    RewriteCond %{REQUEST_URI} !^/var
    RewriteCond %{REQUEST_URI} !^/system
    RewriteCond %{REQUEST_URI} !^/sitemap
    RewriteRule "^/(.*)$" "/content/vcm/us/en/$1.html" [L,PT]

