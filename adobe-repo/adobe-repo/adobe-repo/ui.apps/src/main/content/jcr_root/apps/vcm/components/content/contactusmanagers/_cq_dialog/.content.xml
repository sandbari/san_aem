<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Contact Us Managers Authoring Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[vcm.contactus.authoring]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <contactTeamConfigs
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Primary Contact Details"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <heading
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="To author heading text"
                                                        fieldLabel="Heading"
                                                        name="./heading"/>
                                                    <primaryContact
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Primary Contact Details"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                        margin="{Boolean}true">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <primaryContactDetails
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}true">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                    name="./primaryContactDetails">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <column
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <contactDetails
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                                                    <items jcr:primaryType="nt:unstructured">
                                                                                        <name
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                            fieldDescription="To author primary contact name"
                                                                                            fieldLabel="Name"
                                                                                            name="./primaryContactName"/>
                                                                                        <profilePhoto
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                                            fieldDescription="To author primary contact profile photo"
                                                                                            fieldLabel="Profile Photo"
                                                                                            name="./primaryContactProfilePhoto"
                                                                                            rootPath="/content/dam/vcm"/>
                                                                                        <altText
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                            fieldDescription="To author alt text for primary contact profile photo"
                                                                                            fieldLabel="Alt Text"
                                                                                            name="./primaryContactProfileAltText"
                                                                                            rootPath="/content/dam/vcm"/>
                                                                                        <designation
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                            fieldDescription="To author primary contact designation"
                                                                                            fieldLabel="Designation"
                                                                                            name="./primaryContactDesignation"/>
                                                                                        <phoneNo
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                            fieldDescription="To author primary contact phone number"
                                                                                            fieldLabel="Contact Number"
                                                                                            name="./primaryContactPhoneNo"/>
                                                                                        <mailId
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                            fieldDescription="To author primary contact mailId"
                                                                                            fieldLabel="Mail Id"
                                                                                            name="./primaryContactMailId"/>
                                                                                    </items>
                                                                                </contactDetails>
                                                                            </items>
                                                                        </column>
                                                                    </items>
                                                                </field>
                                                            </primaryContactDetails>
                                                        </items>
                                                    </primaryContact>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </contactTeamConfigs>
                            <secondaryContactDetails
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Secondary Contact Details"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <secondaryContactDetails
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        composite="{Boolean}true"
                                                        fieldLabel="Secondary Contact Details">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            name="./secondaryContactDetails">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <contactDetails
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <name
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                    fieldDescription="To author Secondary contact name"
                                                                                    fieldLabel="Name"
                                                                                    name="./secondaryContactName"/>
                                                                                <phoneNo
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                    fieldDescription="To author Secondary contact phone number"
                                                                                    fieldLabel="Contact Number"
                                                                                    name="./secondaryContactPhoneNo"/>
                                                                                <mailId
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                    fieldDescription="To author Secondary contact mailId"
                                                                                    fieldLabel="Mail Id"
                                                                                    name="./secondaryContactUsMailId"/>
                                                                            </items>
                                                                        </contactDetails>
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                    </secondaryContactDetails>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </secondaryContactDetails>
                            <Audienceselector
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Audience Selector"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <usertype
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        path="vcm/components/content/common/audienceselector"/>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </Audienceselector>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
