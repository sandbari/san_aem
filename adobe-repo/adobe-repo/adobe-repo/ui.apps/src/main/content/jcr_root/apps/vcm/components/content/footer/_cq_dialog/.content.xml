<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Footer Properties"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Footer Configuration Tabs"
                            sling:resourceType="granite/ui/components/coral/foundation/tabs">
                        <items jcr:primaryType="nt:unstructured">
                            <footercontent
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Content"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                            margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <copyrightrext
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="To author copyright text"
                                                            fieldLabel="Copyright Text"
                                                            name="./copyRightText"/>
                                                    <includesocialshare
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            fieldDescription="To include social share component placeholder"
                                                            fieldLabel="Include social share"
                                                            name="./incSocShare"
                                                            text="Include social share"
                                                            value="true"/>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </footercontent>
                            <footerlinks
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Links"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                            margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <footerlinks
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            allowBlank="false"
                                                            composite="{Boolean}true"
                                                            eaem-max-items="8"
                                                            eaem-min-items="1"
                                                            fieldLabel="Footer Links"
                                                            required="{Boolean}true">
                                                        <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                name="./multiFooterLinks">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <linktext
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldDescription="To author link text for the footer link page"
                                                                                fieldLabel="Link text"
                                                                                name="./footerLinkText"
                                                                                required="{Boolean}true"/>
                                                                        <linkurl
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                                fieldDescription="To author link url for the footer link page"
                                                                                fieldLabel="Link Url"
                                                                                name="./footerLinkUrl"
                                                                                required="{Boolean}true"/>
                                                                        <tabselect
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                                fieldDescription="To author target for link to be open in same tab or new tab"
                                                                                fieldLabel="Target"
                                                                                name="./tabSelect">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <sametab
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        text="Same tab"
                                                                                        value="_self"/>
                                                                                <newtab
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        text="New tab"
                                                                                        value="_blank"/>
                                                                            </items>
                                                                        </tabselect>
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                    </footerlinks>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </footerlinks>
                            <footerlogo
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Logo"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true"
                                    required="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                            margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <logodesktabimage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                            fieldDescription="DAM image upload for logo for desktop and tablet"
                                                            fieldLabel="Logo Image for desktop and tablet"
                                                            name="./logoImagePath"
                                                            required="{Boolean}true"
                                                            rootPath="/content/"/>
                                                    <logomobimage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                            fieldDescription="DAM image upload for logo for mobile"
                                                            fieldLabel="Logo Image for mobile"
                                                            name="./logoMobImagePath"
                                                            required="{Boolean}true"
                                                            rootPath="/content/"/>
                                                    <inslogodesktabimage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                            fieldDescription="DAM image upload for logo for desktop and tablet"
                                                            fieldLabel="Institutional Logo Image for desktop and tablet"
                                                            name="./insLogoImagePath"
                                                            rootPath="/content/"/>
                                                    <inslogomobimage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                            fieldDescription="DAM image upload for logo for mobile"
                                                            fieldLabel="Institutional Logo Image for mobile"
                                                            name="./insLogoMobImagePath"
                                                            rootPath="/content/"/>
                                                    <logoalttext
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Image alt text"
                                                            fieldLabel="Logo Alt Text"
                                                            name="./logoAltText"/>
                                                    <logolinkurl
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Link to homepage"
                                                            fieldLabel="Logo link Url"
                                                            name="./logoLinkUrl"/>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </footerlogo>
                            <postloginfooterlinks
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Post Login Links"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <postloginfooterlinks
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        allowBlank="false"
                                                        composite="{Boolean}true"
                                                        eaem-max-items="8"
                                                        eaem-min-items="1"
                                                        fieldLabel="Post Footer Links"
                                                        required="{Boolean}true">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            name="./postLoginMultiFooterLinks">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <linktext
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            fieldDescription="To author link text for the footer link page"
                                                                            fieldLabel="Link text"
                                                                            name="./footerLinkText"
                                                                            required="{Boolean}true"/>
                                                                        <linkurl
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                            fieldDescription="To author link url for the footer link page"
                                                                            fieldLabel="Link Url"
                                                                            name="./footerLinkUrl"
                                                                            required="{Boolean}true"/>
                                                                        <tabselect
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                            fieldDescription="To author target for link to be open in same tab or new tab"
                                                                            fieldLabel="Target"
                                                                            name="./tabSelect">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <sametab
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    text="Same tab"
                                                                                    value="_self"/>
                                                                                <newtab
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    text="New tab"
                                                                                    value="_blank"/>
                                                                            </items>
                                                                        </tabselect>
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                    </postloginfooterlinks>
                                                </items>
                                            </column>
                                        </items>
                                    </columns>
                                </items>
                            </postloginfooterlinks>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
