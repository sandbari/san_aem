<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Quality Structure Authoring Dialog "
          sling:resourceType="cq/gui/components/authoring/dialog"
          extraClientlibs="[cfoverlay.showhidefields]">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Annualized Returns Tabs"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <overlayproperties
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/apps/vcm/components/content/productdetails/configureoverlay/overlayproperties"/>
                    <Advanced
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Advanced"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <variants
                                                    granite:class="cq-dialog-dropdown-showhide"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    fieldLabel="Variant"
                                                    name="./type"
                                                    required="true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <variationtype
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Select Variant Type"
                                                            value=""/>
                                                    <qualitystructure
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Variant 1"
                                                            value="Quality-Structure"/>
                                                    <qualitystructure-two
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Variant 2"
                                                            value="Qualitystructurevariant-two"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-dialog-dropdown-showhide-target=".type-showhide-target"/>
                                            </variants>
                                            <qualityvarianttwo
                                                    granite:class="hide type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <heading
                                                            granite:class="hide type-showhide-target"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Enter the heading"
                                                            fieldLabel="Heading"
                                                            name="./heading">
                                                        <granite:data
                                                                jcr:primaryType="nt:unstructured"
                                                                showhidetargetvalue="Qualitystructurevariant-two"/>
                                                    </heading>
                                                    <asofdate
                                                            granite:class="hide type-showhide-target"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="As of date"
                                                            name="./asOfDate">
                                                        <granite:data
                                                                jcr:primaryType="nt:unstructured"
                                                                showhidetargetvalue="Qualitystructurevariant-two"/>
                                                    </asofdate>
                                                    <tableheadlabel
                                                            granite:class="hide type-showhide-target"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Table Head Label"
                                                            name="./headLabel">
                                                        <granite:data
                                                                jcr:primaryType="nt:unstructured"
                                                                showhidetargetvalue="Qualitystructurevariant-two"/>
                                                    </tableheadlabel>
                                                    <tablebodycontent
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            composite="{Boolean}true"
                                                            fieldLabel="Table Body Content">
                                                        <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                name="./tableBodyContent">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <bodyheading
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldLabel="Body Heading"
                                                                                name="./bodyHeading"/>
                                                                        <bodycontent
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                                                fieldLabel="Body Description"
                                                                                name="./bodyDescription"
                                                                                useFixedInlineToolbar="{Boolean}true">
                                                                            <rtePlugins
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceSuperType="vcm/components/content/common/rtePlugins"/>
                                                                            <uiSettings
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceSuperType="vcm/components/content/common/uiSettings"/>
                                                                        </bodycontent>
                                                                        <fundvalue
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                fieldLabel="Fund Value"
                                                                                name="./fundValue"/>
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                        <granite:data
                                                                jcr:primaryType="nt:unstructured"
                                                                showhidetargetvalue="Qualitystructurevariant-two"/>
                                                    </tablebodycontent>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="Qualitystructurevariant-two"/>
                                            </qualityvarianttwo>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </Advanced>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
