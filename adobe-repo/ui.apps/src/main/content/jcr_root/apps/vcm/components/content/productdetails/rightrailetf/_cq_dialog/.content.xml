<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Right Rail ETF"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <summaryoverlaycontent
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                            fieldDescription="Choose Summary  overlay information from Content Fragments"
                            fieldLabel="Attach Summary overlay content"
                            name="./summaryOverlayContentPath"
                            rootPath="/content/dam/"/>
                    <summaryoverlaytextprefix
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter text for Summary overlay prefix"
                            fieldLabel="Summary Text Prefix"
                            name="./summaryTextPrefix"/>
                    <summaryoverlaylinktext
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter link text for Summary  overlay"
                            fieldLabel="Summary Link Text"
                            name="./summaryLinkText"/>
                    <funddetailsoverlaycontent
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                            fieldDescription="Choose Fund Details information from Content Fragments"
                            fieldLabel="Attach Fund Details overlay content"
                            name="./funddetailsOverlayContentPath"
                            rootPath="/content/dam/"/>
                    <funddetailsoverlaytextprefix
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter text for Fund Details overlay prefix"
                            fieldLabel="Fund Details Text Prefix"
                            name="./funddetailsTextPrefix"/>
                    <funddetailsoverlaylinktext
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter link text for Fund Details overlay"
                            fieldLabel="Fund Details Link Text"
                            name="./funddetailsLinkText"/>
                    <fundyieldoverlaycontent
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                            fieldDescription="Choose Fund Yield information from Content Fragments"
                            fieldLabel="Attach Fund Yield overlay content"
                            name="./fundyieldOverlayContentPath"
                            rootPath="/content/dam/"/>
                    <fundyieldoverlaytextprefix
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter text for Fund Yield overlay prefix"
                            fieldLabel="Fund Yield Text Prefix"
                            name="./fundyieldTextPrefix"/>
                    <fundyieldoverlaylinktext
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Enter link text for Fund Yield overlay"
                            fieldLabel="Fund Yield Link Text"
                            name="./fundyieldLinkText"/>
                    <relatedindextext
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="To author text that appears in the Related Index."
                            fieldLabel="Related Index Text"
                            name="./relatedIndexText"/>
                    <relatedindexurl
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                            fieldDescription="to author the link"
                            fieldLabel="Related Index URL"
                            name="./relatedIndexUrl"
                            rootPath="/content/vcm"/>
                    <linktext
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="To author text that appears in the Link."
                            fieldLabel="Link Text"
                            name="./linkText"/>
                    <linkurl
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                            fieldDescription="to author the link"
                            fieldLabel="Link URL"
                            name="./linkUrl"
                            rootPath="/content/vcm"/>
                    <linkdescription
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="To author the fund literature description "
                            fieldLabel="Fund Literature Description "
                            name="./linkDescription"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
