<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Characteristics Authoring Dialog "
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cfoverlay.showhidefields]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                jcr:title="Annualized Returns Tabs"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <overlayproperties
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/vcm/components/content/productdetails/configureoverlay/overlayproperties"/>
                    <Advanced
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Advanced"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <variants
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Variant"
                                                name="./type"
                                                required="true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <variationtype
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Select Variant Type"
                                                        value=""/>
                                                    <characteristics
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Variant 1"
                                                        value="characteristics"/>
                                                    <characteristics-two
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Variant 2 - Fund For Income"
                                                        value="characteristicsvariant-two"/>
                                                    <characteristics-three
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Variant 3 - Convertibles Fund"
                                                        value="characteristicsvariant-three"/>
                                                </items>
                                            </variants>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </Advanced>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
