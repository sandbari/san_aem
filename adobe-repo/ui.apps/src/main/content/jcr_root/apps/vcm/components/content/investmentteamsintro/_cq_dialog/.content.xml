<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Investment Teams Intro Authoring Dialog "
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <heading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Heading"
                        name="./heading"/>
                    <teams
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        eaem-max-items="5"
                        eaem-min-items="1"
                        fieldLabel="Investment Teams"
                        required="true">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            name="./teamsLeaderBio">
                            <items jcr:primaryType="nt:unstructured">
                                <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                    <items jcr:primaryType="nt:unstructured">
                                        <teamname
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="To author the team name "
                                            fieldLabel="Team Name"
                                            name="./teamName"/>
                                        <biopicture
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                            fieldDescription="To upload DAM image asset for the bio picture "
                                            fieldLabel="Bio Picture"
                                            name="./bioPicture"
                                            rootPath="/content/"/>
                                        <alttext
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="To author the alt text for the picture "
                                            fieldLabel="Alt Text"
                                            name="./altText"/>
                                        <fullname
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="To author full name of the leader "
                                            fieldLabel="Full Name"
                                            name="./fullname"
                                            required="true"/>
                                        <leaderurl
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldDescription="to author link of the leader bio page"
                                            fieldLabel="Leader Bio URL"
                                            name="./leaderUrl"
                                            required="true"
                                            rootPath="/content/vcm"/>
                                        <designation
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="To author the leader's position in the organization (e.g. Senior Portfolio Manager)"
                                            fieldLabel="Designation"
                                            name="./designation"
                                            required="true"/>
                                        <description
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="/libs/cq/gui/components/authoring/dialog/richtext"
                                            fieldDescription="To author the details of the team in the organization "
                                            fieldLabel="Profile Description"
                                            name="./profileDescription"
                                            required="true"
                                            useFixedInlineToolbar="{Boolean}true">
                                            <rtePlugins
												jcr:primaryType="nt:unstructured"
												sling:resourceSuperType="/apps/vcm/components/content/common/rtePlugins"/>
											<uiSettings
												jcr:primaryType="nt:unstructured"
												sling:resourceSuperType="/apps/vcm/components/content/common/uiSettings"/>
                                        </description>
                                        <linktext
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="To author link text for 'View Teams' in the Investment team. "
                                            fieldLabel="Link Text"
                                            name="./linkText"
                                            required="true"/>
                                        <linkurl
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldDescription="to author the link of the team"
                                            fieldLabel="Link URL"
                                            name="./linkUrl"
                                            required="true"
                                            rootPath="/content/vcm"/>
                                        <linkdescription
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="To author the aria-label or link description of the link. "
                                            fieldLabel="Link Description"
                                            name="./linkDescription"/>
										<tabselect
											jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/select"
											fieldDescription="To author target for link to be open in same tab or new tab"
											fieldLabel="Target"
											name="./linkTarget">
											<items jcr:primaryType="nt:unstructured">
												<sametab
													jcr:primaryType="nt:unstructured"
													text="Same tab"
													value="_self"/>
												<newtab
													jcr:primaryType="nt:unstructured"
													text="New tab"
													value="_blank"/>
											</items>
										</tabselect>
                                    </items>
                                </column>
                            </items>
                        </field>
                    </teams>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
