<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Contact Victory"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cfoverlay.showhidefields]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        granite:class="cq-RichText-FixedColumn-column"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <bgImageDesktopShow
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="To upload DAM image for desktop view"
                                                fieldLabel="Background Image Desktop View"
                                                name="./bgImageDesktopShow"
                                                required="{Boolean}true"
                                                rootPath="/content/dam/vcm"
                                                wrapperClass="cmp-image__editor-link"/>
                                            <bgImageMobileShow
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldDescription="To upload DAM image for mobile view"
                                                fieldLabel="Background Image Mobile View"
                                                name="./bgImageMobileShow"
                                                required="{Boolean}true"
                                                rootPath="/content/dam/vcm"
                                                wrapperClass="cmp-image__editor-link"/>
                                            <altText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Author alt text of the contact us page"
                                                fieldLabel="Alt Text"
                                                name="./altText"/>
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Author heading text here"
                                                fieldLabel="Heading"
                                                name="./heading"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Author field description here"
                                                fieldLabel="Description"
                                                name="./description"
                                                removeSingleParagraphContainer="{Boolean}true"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="vcm/components/content/common/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="vcm/components/content/common/uiSettings"/>
                                            </description>
                                            <linktext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Author link text of the contact us page"
                                                fieldLabel="Link Text"
                                                name="./linkText"
                                                required="{Boolean}true"/>
                                            
                                            <linkURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="To author link of the contact us page "
                                                fieldLabel="Link Url"
                                                name="./linkUrl"
                                                required="{Boolean}true"
                                                rootPath="/content/vcm"
                                                wrapperClass="cmp-image--editor-link"/>
                                            <tabselect
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="To author target for link to be open in same tab or new tab"
                                                fieldLabel="Target"
                                                name="./tabSelect">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <sametab
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Same tab"
                                                        value="_self"/>
                                                    <newtab
                                                        jcr:primaryType="nt:unstructured"
                                                        text="New tab"
                                                        value="_blank"/>
                                                </items>
                                            </tabselect>
                                            <iscorphomepage
                                                    granite:class="cq-dialog-checkbox-showhide"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                    fieldLabel="Is Corp Homepage"
                                                    name="./isCorpHomepage"
                                                    text="Is Corp Homepage"
                                                    uncheckedValue="false"
                                                    value="true">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-dialog-checkbox-showhide-target=".enable-overlay-showhide-target"/>
                                            </iscorphomepage>
                                            <togglefields
                                                    granite:class="enable-overlay-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <contactLinkUrlFinancial
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Link to ‘Contact Us’ page"
                                                            fieldLabel="Contact link URL for Financial Advisor"
                                                            name="./contactLinkUrlFinancial"/>
                                                    <contactLinkUrlII
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Link to ‘Contact Us’ page"
                                                            fieldLabel="Contact link URL for institutional investor"
                                                            name="./contactLinkUrlII"/>
                                                </items>
                                            </togglefields>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <Audienceselector
                       jcr:primaryType="nt:unstructured"
                       jcr:title="Audience Selector"
                       sling:resourceType="granite/ui/components/coral/foundation/container"
                       margin="{Boolean}true">
                       <items jcr:primaryType="nt:unstructured">
                           <columns
                               jcr:primaryType="nt:unstructured"
                               sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                               margin="{Boolean}true">
                               <items jcr:primaryType="nt:unstructured">
                                   <column
                                       jcr:primaryType="nt:unstructured"
                                       sling:resourceType="granite/ui/components/coral/foundation/container">
                                       <items jcr:primaryType="nt:unstructured">
                                           <usertype
                                               jcr:primaryType="nt:unstructured"
                                               sling:resourceType="granite/ui/components/foundation/include"
                                               path="vcm/components/content/common/audienceselector"/>
                                       </items>
                                   </column>
                               </items>
                           </columns>
                       </items>
                   </Audienceselector>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
